# JUPYTER NOTEBOOK: WORKER TASK ANALYSIS
# Analyze worker performance metrics from results.json

# %%
# Import required libraries
import json

import matplotlib.pyplot as plt
import numpy as np
import pandas as pd
import seaborn as sns

# Set plotting style
sns.set_style("whitegrid")
plt.rcParams["figure.figsize"] = (14, 8)

# %%
# 1. LOAD DATA
# Read results from JSON file
with open("results.json", "r") as f:
    data = json.load(f)

# Convert to DataFrame
df = pd.DataFrame(data)

# Display basic info
print(f"Total tasks: {len(df)}")
print("\nFirst few rows:")
print(df.head())

# %%
# 2. DATA PREPARATION
# Calculate latency for each task
df["latency_ms"] = df["completedAtMs"] - df["startedAtMs"]

# Convert completion time to seconds (rounded down for grouping)
df["completion_second"] = np.floor(df["completedAtMs"] / 1000).astype(int)

# Convert failed to numeric for calculations
df["success"] = (~df["failed"]).astype(int)

print("\nProcessed data:")
print(df.head())

# %%
# 3. AGGREGATE METRICS BY SECOND
# Group by completion second and calculate metrics
aggregated = (
    df.groupby("completion_second")
    .agg(
        {
            "worker_id": "nunique",  # Number of unique workers
            "success": ["mean", "sum"],  # Success rate and count
            "latency_ms": [
                "mean",
                lambda x: np.percentile(x, 99),
                "count",
            ],  # Avg latency, P99, count
        }
    )
    .reset_index()
)

# Flatten column names
aggregated.columns = [
    "completion_second",
    "num_workers",
    "success_rate",
    "success_count",
    "avg_latency_ms",
    "p99_latency_ms",
    "total_tasks",
]

# Convert success rate to percentage
aggregated["success_percentage"] = aggregated["success_rate"] * 100

print("\nAggregated metrics by second:")
print(aggregated)

# %%
# 4. VISUALIZATION: TASKS COMPLETED PER SECOND
fig, ax = plt.subplots(figsize=(14, 5))
ax.plot(
    aggregated["completion_second"],
    aggregated["total_tasks"],
    color="steelblue",
    alpha=0.8,
    linewidth=1.5,
)
ax.fill_between(
    aggregated["completion_second"],
    aggregated["total_tasks"],
    color="steelblue",
    alpha=0.3,
)
ax.set_xlabel("Completion Time (seconds)", fontsize=12)
ax.set_ylabel("Number of Tasks Completed", fontsize=12)
ax.set_title("Task Completion Distribution Over Time", fontsize=14, fontweight="bold")
ax.grid(axis="y", alpha=0.3)
plt.tight_layout()
plt.show()

# %%
# 5. VISUALIZATION: SUCCESS RATE OVER TIME
fig, ax = plt.subplots(figsize=(14, 5))
ax.plot(
    aggregated["completion_second"],
    aggregated["success_percentage"],
    linewidth=1.5,
    color="green",
    alpha=0.8,
)
ax.set_xlabel("Completion Time (seconds)", fontsize=12)
ax.set_ylabel("Success Rate (%)", fontsize=12)
ax.set_title("Task Success Rate Over Time", fontsize=14, fontweight="bold")
ax.set_ylim([0, 105])
ax.axhline(y=100, color="red", linestyle="--", alpha=0.5, label="100% Success")
ax.legend()
ax.grid(alpha=0.3)
plt.tight_layout()
plt.show()

# %%
# 6. VISUALIZATION: LATENCY METRICS OVER TIME
fig, ax = plt.subplots(figsize=(14, 5))
ax.plot(
    aggregated["completion_second"],
    aggregated["avg_latency_ms"],
    linewidth=1.5,
    label="Average Latency",
    color="blue",
    alpha=0.8,
)
ax.plot(
    aggregated["completion_second"],
    aggregated["p99_latency_ms"],
    linewidth=1.5,
    label="P99 Latency",
    color="red",
    alpha=0.8,
)
ax.set_xlabel("Completion Time (seconds)", fontsize=12)
ax.set_ylabel("Latency (ms)", fontsize=12)
ax.set_title("Task Latency Metrics Over Time", fontsize=14, fontweight="bold")
ax.legend()
ax.grid(alpha=0.3)
plt.tight_layout()
plt.show()

# %%
# 7. VISUALIZATION: NUMBER OF ACTIVE WORKERS OVER TIME
fig, ax = plt.subplots(figsize=(14, 5))
ax.plot(
    aggregated["completion_second"],
    aggregated["num_workers"],
    linewidth=1.5,
    color="purple",
    alpha=0.8,
)
ax.set_xlabel("Completion Time (seconds)", fontsize=12)
ax.set_ylabel("Number of Unique Workers", fontsize=12)
ax.set_title("Active Workers Over Time", fontsize=14, fontweight="bold")
ax.grid(alpha=0.3)
plt.tight_layout()
plt.show()

# %%
# 8. COMPREHENSIVE DASHBOARD: ALL METRICS
fig, axes = plt.subplots(2, 2, figsize=(18, 10))

# Subplot 1: Tasks completed
axes[0, 0].plot(
    aggregated["completion_second"],
    aggregated["total_tasks"],
    color="steelblue",
    alpha=0.8,
    linewidth=1.5,
)
axes[0, 0].fill_between(
    aggregated["completion_second"],
    aggregated["total_tasks"],
    color="steelblue",
    alpha=0.3,
)
axes[0, 0].set_xlabel("Completion Time (seconds)")
axes[0, 0].set_ylabel("Tasks Completed")
axes[0, 0].set_title("Tasks Completed Per Second", fontweight="bold")
axes[0, 0].grid(axis="y", alpha=0.3)

# Subplot 2: Success rate
axes[0, 1].plot(
    aggregated["completion_second"],
    aggregated["success_percentage"],
    linewidth=1.5,
    color="green",
    alpha=0.8,
)
axes[0, 1].set_xlabel("Completion Time (seconds)")
axes[0, 1].set_ylabel("Success Rate (%)")
axes[0, 1].set_title("Success Rate Over Time", fontweight="bold")
axes[0, 1].set_ylim([0, 105])
axes[0, 1].grid(alpha=0.3)

# Subplot 3: Latency metrics
axes[1, 0].plot(
    aggregated["completion_second"],
    aggregated["avg_latency_ms"],
    linewidth=1.5,
    label="Avg Latency",
    color="blue",
    alpha=0.8,
)
axes[1, 0].plot(
    aggregated["completion_second"],
    aggregated["p99_latency_ms"],
    linewidth=1.5,
    label="P99 Latency",
    color="red",
    alpha=0.8,
)
axes[1, 0].set_xlabel("Completion Time (seconds)")
axes[1, 0].set_ylabel("Latency (ms)")
axes[1, 0].set_title("Latency Metrics Over Time", fontweight="bold")
axes[1, 0].set_ylim([0, 5000])
axes[1, 0].legend()
axes[1, 0].grid(alpha=0.3)

# Subplot 4: Active workers
axes[1, 1].plot(
    aggregated["completion_second"],
    aggregated["num_workers"],
    linewidth=1.5,
    color="purple",
    alpha=0.8,
)
axes[1, 1].set_xlabel("Completion Time (seconds)")
axes[1, 1].set_ylabel("Number of Workers")
axes[1, 1].set_title("Active Workers Over Time", fontweight="bold")
axes[1, 1].grid(alpha=0.3)

plt.savefig("plot.png", dpi=300, bbox_inches="tight")
plt.tight_layout()
plt.show()

# %%
# 9. SUMMARY STATISTICS
print("\n" + "=" * 60)
print("SUMMARY STATISTICS")
print("=" * 60)
print("\nOverall Metrics:")
print(f"  Total Tasks: {len(df)}")
print(f"  Success Rate: {df['success'].mean()*100:.2f}%")
print(f"  Average Latency: {df['latency_ms'].mean():.2f} ms")
print(f"  P99 Latency: {np.percentile(df['latency_ms'], 99):.2f} ms")
print(f"  Unique Workers: {df['worker_id'].nunique()}")
print("\nTime Range:")
print(f"  First completion: {df['completedAtMs'].min():.2f} ms")
print(f"  Last completion: {df['completedAtMs'].max():.2f} ms")
print(
    f"  Duration: {(df['completedAtMs'].max() - df['completedAtMs'].min()) / 1000:.2f} seconds"
)

